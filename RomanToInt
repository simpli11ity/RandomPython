class Solution:
        def romanToInt(self, s: str) -> int:

            Roman = {
            "I": 1,
            "V": 5,
            "X": 10,
            "L": 50,
            "C": 100,
            "D": 500,
            "M": 1000,
            "IV":4,
            "IX":9,
            "XL":40,
            "XC":90,
            "CD":400,
            "CM":900
            }

            Total=0
            i=0
            while i < len(s):
                if i+1<=len(s) and s[i:i+2] in Roman:
                   
                    Total+=Roman[s[i:i+2]]
                    i=i+2
                 
                elif i<=len(s):
                  
                    Total+=Roman[s[i]]
                    i=i+1
              
            
            return(Total)
