
//getting user input
n = int(input())

//checks if n is even and in range of 5-21 2-20 inclusively and prints "weird"
if n%2==1 or (n%2==0 and n in range(5,21)):
    print('Weird')

//checks number is even and less than 20 or if its even and in the range or 1-6
elif n%2==0 and n>20 or n%2==0 and n in range(1,6):
    print('Not Weird')
